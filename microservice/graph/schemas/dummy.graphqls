extend type Query {
	DummyByID(id: Int!): Dummy @auth
	Dummies(
		first: Int
		after: Cursor
		last: Int
		before: Cursor
		orderBy: DummyOrder
		where: DummyWhereInput
	): DummyConnection! @auth
}

extend type Mutation {
	createDummy(input: CreateDummyInput!): Dummy! @auth
	createBulkDummy(input: [CreateDummyInput!]!): [Dummy!]! @auth
	updateDummy(id: Int!, input: UpdateDummyInput!): Dummy! @auth
	deleteDummy(id: Int!): Boolean! @auth
}
